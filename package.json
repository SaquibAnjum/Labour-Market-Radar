{
  "name": "labour-market-radar",
  "version": "1.0.0",
  "description": "MERN Labour Market Analytics Dashboard",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "playwright": "^1.37.0",
    "cheerio": "^1.0.0-rc.12",
    "bullmq": "^4.12.0",
    "ioredis": "^5.3.2",
    "agenda": "^5.0.0",
    "crypto": "^1.0.1",
    "axios": "^1.5.0",
    "node-cron": "^3.0.2",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3"
  },
  "keywords": ["mern", "labour-market", "analytics", "dashboard", "scraping"],
  "author": "Labour Market Radar Team",
  "license": "MIT"
}
